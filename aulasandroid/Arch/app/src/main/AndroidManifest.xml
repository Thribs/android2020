<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.digitalhouse.arch">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_measure"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_measure"
        android:supportsRtl="true"
        android:theme="@style/Theme.Arch">
        <activity android:name=".ui.products.NewProductActivity"></activity>
        <activity android:name=".ui.products.CartActivity" />
        <activity android:name=".ui.products.ProductsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>